function createNewUser() {
    let newUser;

    newUser = {     // an object
        firstName: prompt('Name'),
        lastName: prompt('Surname')
    };
    fletter=newUser.firstName.charAt(0);
    surname=newUser.lastName;

    alert(this.fletter + ' ' + this.surname);

}


createNewUser();
